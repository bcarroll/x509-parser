{
	"name": "x509-parser",
	"displayName": "x509 parser",
	"description": "Parse x509 certificates to human readable text",
	"version": "0.0.1",
	"publisher": "jlcs-es",
	"repository": {
		"type": "git",
		"url": "https://github.com/jlcs-es/x509-parser"
	},
	"bugs": {
		"url": "https://github.com/jlcs-es/x509-parser/issues"
	},
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.parsePEM"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.parsePEM",
				"title": "Parse x509 PEM"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"openssl-commander": "^0.1.2"
	}
}
